{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "heading": {
      "type": "string",
      "required": true
    },
    "journalist": {
      "type": "string",
      "required": true
    },
    "body": {
      "type": "richtext",
      "required": true
    },
    "DatePublished": {
      "type": "datetime"
    },
    "Media": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Sports",
        "Business",
        "Weather",
        "Regular news"
      ],
      "required": true,
      "default": "Regular news"
    }
  }
}
